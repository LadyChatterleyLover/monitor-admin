{
  "name": "caiyun",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview --port 4173",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.md,.json --max-warnings 0 && pretty-quick --check",
    "lint:fix": "eslint --fix . --ext .vue,.js,.ts,.jsx,.tsx,.md,.json && pretty-quick",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@commitlint/cli": "^17.3.0",
    "@commitlint/config-conventional": "^17.3.0",
    "axios": "^1.2.6",
    "cz-customizable": "^7.0.0",
    "pinia": "^2.0.21",
    "qs": "^6.11.0",
    "vue": "^3.2.38",
    "vue-router": "^4.1.5",
    "vxe-table": "^4.3.9",
    "xe-utils": "^3.5.7"
  },
  "devDependencies": {
    "@arco-design/web-vue": "^2.42.0",
    "@element-plus/eslint-config": "^0.0.20221124",
    "@types/node": "^16.11.56",
    "@types/qs": "^6.9.7",
    "@vitejs/plugin-vue": "^3.0.3",
    "@vue/tsconfig": "^0.1.3",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.28.0",
    "husky": "^8.0.2",
    "lint-staged": "^13.0.3",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.19",
    "prettier": "^2.8.0",
    "pretty-quick": "^3.1.3",
    "sass": "^1.57.1",
    "tailwindcss": "^3.2.4",
    "typescript": "~4.7.4",
    "vite": "^3.0.9",
    "vue-tsc": "^0.40.7"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    }
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,md,json}": "eslint --fix"
  }
}
